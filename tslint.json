{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-sonarts"
  ],
  "linterOptions": {
    "exclude": [
      "**/*.spec.ts",
      "**/*.d.ts",
      "jest.environment.ts",
      "test"
    ]
  },
  "jsRules": {},
  "rules": {
    "no-extra-semicolon": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
